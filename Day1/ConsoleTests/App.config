<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section 
      name="RegisterServices" 
      type="DomainConfig.CustomSections.ServiceConfig.ServiceConfigSection, DomainConfig"/>
    <section 
      name="MasterDependencies" 
      type="DomainConfig.CustomSections.DependencyConfig.DependencyConfigSection, DomainConfig"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="Path" value="./users.xml"/>
  </appSettings>
  <system.diagnostics>
    <switches>
      <add name="Data" value="1" />
    </switches>
  </system.diagnostics>
  <RegisterServices>
    <Services>
      <add serviceType="Master" serviceName="master" />
      <add serviceType="Slave" serviceName="slave_1" ip ="127.0.0.1" port="9000"/>
      <add serviceType="Slave" serviceName="slave_2" ip ="127.0.0.1" port="9001"/>
      <add serviceType="Slave" serviceName="slave_3" ip ="127.0.0.1" port="9002"/>
    </Services>
  </RegisterServices>
  <MasterDependencies>
    <MasterService name="master">
      <add name="slave_1" ip ="127.0.0.1" port="9000"/>
      <add name="slave_2" ip ="127.0.0.1" port="9001"/>
      <add name="slave_3" ip ="127.0.0.1" port="9002"/>
    </MasterService>
  </MasterDependencies>
</configuration>