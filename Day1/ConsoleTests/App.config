<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section 
      name="RegisterServices" 
      type="DomainConfig.CustomConfigSections.RegisterServices, DomainConfig"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="Path" value="./users.xml"/>
    <add key="SlavesNumber" value="5"/>
  </appSettings>
  <system.diagnostics>
    <switches>
      <add name="Data" value="1" />
    </switches>
  </system.diagnostics>
  <RegisterServices>
    <Services>
      <add serviceType="Master" serviceName="master" />
      <add serviceType="Slave" serviceName="slave_1" />
      <add serviceType="Slave" serviceName="slave_2" />
    </Services>
  </RegisterServices> 
</configuration>